extends layout  
block content
  h1= titulo
  table
    thead
      tr
        th Nombre
        th Apellido
        th DNI
        th Fecha Nacimiento
        th Sexo
    tbody
      each paciente in pacientes
        tr
          td= paciente.nombre
          td= paciente.apellido
          td= paciente.dni
          td= paciente.fecha_nacimiento
          td= paciente.sexo